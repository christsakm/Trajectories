@@ -0,0 +1,39 @@
+from typing import Generic, TypeVar
+T = TypeVar('T')
+U = TypeVar('U')
+
+class C(Generic[T]):
+    @staticmethod
+    def static_method() -> 'C[T]':
+        return C()
+    
+    @classmethod
+    def class_method(cls) -> 'C[T]':
+        return cls()
+        
+    def instance_method(self) -> 'C[T]':
+        return C()
+
+class D(C[T], Generic[T, U]):
+    @staticmethod
+    def static_method_d() -> 'D[T, U]':
+        return D()
+
+# Test cases
+# 1. Basic static method on generic class
+x1: C[int] = C[int].static_method()
+
+# 2. Class method (should still work)
+x2: C[int] = C[int].class_method()
+
+# 3. Instance method (should still work)
+x3: C[int] = C[int]().instance_method()
+
+# 4. Static method on derived generic class with multiple type vars
+x4: D[int, str] = D[int, str].static_method_d()
+
+# 5. Function with type variables
+def f(t: T) -> C[T]:
+    return C[T].static_method()
+
+print("All tests passed!")
