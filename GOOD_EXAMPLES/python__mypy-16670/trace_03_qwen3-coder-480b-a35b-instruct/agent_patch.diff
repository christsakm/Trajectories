@@ -0,0 +1,23 @@
+#!/usr/bin/env python3
+
+"""Debug script to understand the issue with staticmethod on generic types."""
+
+from typing import Generic, TypeVar
+T = TypeVar('T')
+
+class C(Generic[T]):
+    @staticmethod
+    def c() -> 'C[T]':
+        return C()
+
+def f(t: T) -> C[T]:
+    # This should work - calling staticmethod on a parameterized generic type
+    result = C[T].c()
+    print(f"Type of result: {type(result)}")
+    return result
+
+# Test the function
+if __name__ == "__main__":
+    # This should work at runtime
+    x = f(42)
+    print(f"Result: {x}")
