+    def _sort_json_schema(self, schema: JsonSchemaValue) -> JsonSchemaValue:
+        """
+        Recursively sort a JSON schema to ensure deterministic ordering.
+        """
+        if isinstance(schema, dict):
+            # Sort dictionary keys
+            sorted_dict = {}
+            for key in sorted(schema.keys()):
+                value = schema[key]
+                # Special handling for 'required' field which should be sorted as a list
+                if key == 'required' and isinstance(value, list):
+                    sorted_dict[key] = sorted(value)
+                # Special handling for 'anyOf', 'allOf', 'oneOf' which are lists of schemas
+                elif key in ('anyOf', 'allOf', 'oneOf') and isinstance(value, list):
+                    sorted_dict[key] = [self._sort_json_schema(item) for item in value]
+                # Sort other values recursively
+                else:
+                    sorted_dict[key] = self._sort_json_schema(value)
+            return sorted_dict
+        elif isinstance(schema, list):
+            # For lists of schemas, sort them based on their string representation
+            # This ensures deterministic ordering for lists like those in 'anyOf'
+            if all(isinstance(item, dict) for item in schema):
+                # Sort list items by their string representation
+                return sorted(
+                    [self._sort_json_schema(item) for item in schema],
+                    key=lambda x: json.dumps(x, sort_keys=True)
+                )
+            else:
+                # For other lists, just sort the items recursively
+                return [self._sort_json_schema(item) for item in schema]
+        else:
+            # For primitive values, return as is
+            return schema
